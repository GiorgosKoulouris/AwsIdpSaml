<EntityDescriptor xmlns="urn:oasis:names:tc:SAML:2.0:metadata" 
        entityID="https://my-domain.com" 
        validUntil="y-m-dTH:M:SZ">
    <IDPSSODescriptor xmlns:ds="http://www.w3.org/2000/09/xmldsig#" protocolSupportEnumeration="urn:oasis:names:tc:SAML:2.0:protocol">
        <KeyDescriptor use="signing">
            <ds:KeyInfo>
                <ds:X509Data>
                    <ds:X509Certificate>CERT_REDUCTED
                    </ds:X509Certificate>
                </ds:X509Data>
            </ds:KeyInfo>
        </KeyDescriptor>
        <SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect" Location="https://my-domain.com"/>
    </IDPSSODescriptor>
</EntityDescriptor>
